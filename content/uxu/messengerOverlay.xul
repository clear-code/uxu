<?xml version="1.0"?>
<!DOCTYPE overlay SYSTEM "chrome://uxu/locale/uxu.dtd">
<overlay id="uxu-overlay"
	xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">

<script src="chrome://global/content/nsUserSettings.js"/>
<script src="chrome://uxu/content/browserOverlay.js"/>

<menupopup id="taskPopup">
	<menu id="tools-uxu-menu"
		insertbefore="devToolsSeparator"
		label="&uxu.tools.menu.label;">
		<menupopup>
			<menuitem id="tools-uxu-menuitem"
				label="&uxu.tools.start.label;"
				oncommand="startUxU()"/>
			<menuitem id="tools-uxu-menuitem-mozunit"
				label="&uxu.tools.mozunit.label;"
				oncommand="openUxUMozUnit()"/>
		</menupopup>
	</menu>
</menupopup>

</overlay>
